{
  "name": "angular-highcharts-declarative",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Anton Simola",
  "description": "Angular >13.0,  Highcharts 9 wrapper with declarative configuration + reactive inputs",
  "repository": "https://github.com/antonsimola/angular-highcharts-declarative",
  "scripts": {
    "ng": "ng",
    "copyfiles": "copyfiles",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier --write projects/**/*.ts",
    "copy:readme": "copyfiles ./README.md ./dist/angular-highcharts-declarative",
    "copy:materialhighcharts": "copyfiles --flat  ./projects/angular-highcharts-declarative/src/lib/styles/* ./dist/angular-highcharts-declarative/styles",
    "build:lib": "ng build angular-highcharts-declarative --configuration production && npm run copy:readme && npm run copy:materialhighcharts",
    "build:showcase": "ng build --configuration production angular-highcharts-declarative-showcase --base-href=\"https://antonsimola.github.io/angular-highcharts-declarative/\"",
    "publish:showcase": "npm run build:showcase && npx angular-cli-ghpages --dir=./dist/angular-highcharts-declarative-showcase",
    "publish:lib": "npm run build:lib && npm publish ./dist/angular-highcharts-declarative"
  },
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.11",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "highcharts": "10.1.0",
    "ngx-take-until-destroy": "^5.4.0",
    "prismjs": "1.24.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular-devkit/core": "^13.3.8",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/language-service": "~13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.6",
    "angular-cli-ghpages": "^0.6.0-rc.2",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.6.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.1",
    "npx": "^10.2.2",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
