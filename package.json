{
  "name": "angular-highcharts-declarative",
  "version": "17.0.1",
  "license": "MIT",
  "author": "Anton Simola",
  "description": "Angular >17.0,  Highcharts 10 wrapper with declarative configuration + reactive inputs",
  "repository": "https://github.com/antonsimola/angular-highcharts-declarative",
  "scripts": {
    "ng": "ng",
    "copyfiles": "copyfiles",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier --write projects/**/*.ts",
    "copy:readme": "copyfiles ./README.md ./dist/angular-highcharts-declarative",
    "copy:materialhighcharts": "copyfiles --flat  ./projects/angular-highcharts-declarative/src/lib/styles/* ./dist/angular-highcharts-declarative/styles",
    "build:lib": "ng build angular-highcharts-declarative --configuration production && npm run copy:readme && npm run copy:materialhighcharts",
    "build:showcase": "ng build --configuration production angular-highcharts-declarative-showcase --base-href=\"https://antonsimola.github.io/angular-highcharts-declarative/\"",
    "publish:showcase": "npm run build:showcase && npx angular-cli-ghpages --dir=./dist/angular-highcharts-declarative-showcase",
    "publish:lib": "npm run build:lib && npm publish ./dist/angular-highcharts-declarative"
  },
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "highcharts": "10.1.0",
    "ngx-take-until-destroy": "^5.4.0",
    "prismjs": "1.24.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.0",
    "@angular-devkit/core": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.6",
    "angular-cli-ghpages": "^0.6.0-rc.2",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.6.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^20.1.0",
    "npx": "^10.2.2",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5"
  }
}
