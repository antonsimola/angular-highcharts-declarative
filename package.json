{
  "name": "angular-highcharts-declarative",
  "version": "0.0.20",
  "license": "MIT",
  "author": "Anton Simola",
  "description": "Angular >=8.0 Highcharts wrapper with declarative configuration + reactive inputs",
  "repository": "https://github.com/antonsimola/angular-highcharts-declarative",
  "scripts": {
    "ng": "ng",
    "copyfiles": "copyfiles",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier --write projects/**/*.ts",
    "copy:readme": "copyfiles ./README.md ./dist/angular-highcharts-declarative",
    "copy:materialhighcharts": "copyfiles --flat  ./projects/angular-highcharts-declarative/src/lib/styles/* ./dist/angular-highcharts-declarative/styles",
    "build:lib": "ng build angular-highcharts-declarative && npm run copy:readme && npm run copy:materialhighcharts",
    "build:showcase": "ng build --prod angular-highcharts-declarative-showcase --base-href=\"https://antonsimola.github.io/angular-highcharts-declarative/\"",
    "publish:showcase": "npm run build:showcase && npx angular-cli-ghpages --dir=./dist/angular-highcharts-declarative-showcase",
    "publish:lib": "npm run build:lib && npm publish ./dist/angular-highcharts-declarative"
  },
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "highcharts": "^7.1.3",
    "ngx-take-until-destroy": "^5.4.0",
    "prismjs": "^1.9.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular-devkit/core": "^12.2.0",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@bazel/typescript": "^0.35.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.9.0",
    "angular-cli-ghpages": "^0.6.0-rc.2",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "npx": "^10.2.0",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  }
}
