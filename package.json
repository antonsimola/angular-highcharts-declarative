{
  "name": "angular-highcharts-declarative",
  "version": "0.0.18",
  "license": "MIT",
  "author": "Anton Simola",
  "description": "Angular >=8.0 Highcharts wrapper with declarative configuration + reactive inputs",
  "repository": "https://github.com/antonsimola/angular-highcharts-declarative",
  "scripts": {
    "ng": "ng",
    "copyfiles": "copyfiles",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier --write projects/**/*.ts",
    "copy:readme": "copyfiles ./README.md ./dist/angular-highcharts-declarative",
    "copy:materialhighcharts": "copyfiles --flat  ./projects/angular-highcharts-declarative/src/lib/styles/* ./dist/angular-highcharts-declarative/styles",
    "build:lib": "ng build angular-highcharts-declarative && npm run copy:readme && npm run copy:materialhighcharts",
    "build:showcase": "ng build --prod angular-highcharts-declarative-showcase --base-href=\"https://antonsimola.github.io/angular-highcharts-declarative/\"",
    "publish:showcase": "npm run build:showcase && npx angular-cli-ghpages --dir=./dist/angular-highcharts-declarative-showcase",
    "publish:lib": "npm run build:lib && npm publish ./dist/angular-highcharts-declarative"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.1.2",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "highcharts": "^7.1.3",
    "ngx-take-until-destroy": "^5.4.0",
    "prismjs": "^1.9.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@bazel/typescript": "^0.35.0",
    "@types/prismjs": "^1.9.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "copyfiles": "^2.1.1",
    "angular-cli-ghpages": "^0.6.0-rc.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "npx": "^10.2.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
