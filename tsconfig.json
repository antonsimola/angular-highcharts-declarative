{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types",
      "node"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "angular-highcharts-declarative": [
        "dist/angular-highcharts-declarative"
      ],
      "angular-highcharts-declarative/*": [
        "dist/angular-highcharts-declarative/*"
      ]
    }
  }
}
